    <rules>
        <fullName>Approved by GM w%2Fo</fullName>
        <actions>
            <name>Update_Vetting_Stage</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( OR(   RecordType.DeveloperName = &quot;TW1_Vendor_Vetting&quot;,   RecordType.DeveloperName = &quot;TW3_Vendor_Vetting_Senegal&quot; ),  ISCHANGED( TW1_Approval_Status_GM_SC_FIN_CON__c), ISPICKVAL(TW1_Approval_Status_GM_SC_FIN_CON__c, &quot;Approved&quot;),  TW1_Is_RedFlagged__c = FALSE)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
