    <fullName>StatusIcon__c</fullName>
    <externalId>false</externalId>
    <formula>IF(Text(Status__c) = &apos;Pending&apos;, &apos;----&apos;,  
  IF(Text(Status__c) = &apos;Supported&apos;, IMAGE(&quot;/resource/support/supported1.png&quot;, &quot;Supported&quot;, 40, 40),
      IMAGE(&quot;/resource/support/notsupported1.png&quot;, &quot;Not Supported&quot;, 30, 30)
   )
)</formula>
