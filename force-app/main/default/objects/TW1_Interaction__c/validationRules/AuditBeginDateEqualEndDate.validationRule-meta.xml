<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AuditBeginDateEqualEndDate</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
				TW1_Beginning_Date__c = TW1_End_Date__c,
				 RecordType.DeveloperName = &quot;Audit&quot;
				)</errorConditionFormula>
    <errorMessage>You can&apos;t schedule an audit with with the same Beginning and End date.</errorMessage>
</ValidationRule>
