<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>finishDateValidaton</fullName>
    <active>false</active>
    <errorConditionFormula>AND(OR(ISCHANGED(  EstimatedFinishDate__c  ), ISNEW()), EstimatedFinishDate__c &lt; TODAY())</errorConditionFormula>
    <errorDisplayField>EstimatedFinishDate__c</errorDisplayField>
    <errorMessage>The field &apos;Estimated Finish Date&apos; can not be in the past</errorMessage>
</ValidationRule>
