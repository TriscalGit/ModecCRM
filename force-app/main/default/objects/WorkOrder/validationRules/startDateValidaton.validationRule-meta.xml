<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>startDateValidaton</fullName>
    <active>false</active>
    <errorConditionFormula>AND(OR(ISCHANGED( EstimatedStartDate__c ), ISNEW()), EstimatedStartDate__c &lt; TODAY())</errorConditionFormula>
    <errorDisplayField>EstimatedStartDate__c</errorDisplayField>
    <errorMessage>The field &apos;Estimated Start Date&apos; can not be in the past</errorMessage>
</ValidationRule>
