<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MultipleUser</fullName>
    <active>true</active>
    <errorConditionFormula>AND(MultipleUser__c, ISBLANK(Owner:Queue.Id))</errorConditionFormula>
    <errorDisplayField>MultipleUser__c</errorDisplayField>
    <errorMessage>To multiple user task you must select a queue</errorMessage>
</ValidationRule>
