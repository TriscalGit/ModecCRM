<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TW2_Duration_in_Months__c</fullName>
    <externalId>false</externalId>
    <formula>MONTH(TW1_End_Date__c) - MONTH(TW1_Start_Date__c) + 12*(YEAR(TW1_End_Date__c) - YEAR(TW1_Start_Date__c)) - IF(DAY(TW1_End_Date__c) &lt; DAY(TW1_Start_Date__c),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Duration in Months</label>
    <precision>18</precision>
    <required>false</required>
