        <field>LessonLearnedList__c</field>
        <field>RootCause__c</field>
        <field>FinalBearer__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>ClosedOutApprovalNotification</name>
                <type>Alert</type>
            </action>
            <action>
                <name>CloseOut_PEM</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>EngineeringManager</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>NAM__c.SkipEM__c</field>
                <operation>equals</operation>
                <value>False</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>GotoNextStep</ifCriteriaNotMet>
        <label>EM</label>
        <name>EM</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>ClosedOut_TeachTeam</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>ClosedOutApprovalNotification</name>
                <type>Alert</type>
            </action>
            <action>
                <name>ClosedOutClarification_true</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>COMdBApproval</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>PEM</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>1 or 2</booleanFilter>
            <criteriaItems>
                <field>NAM__c.ClosedOutStatus__c</field>
                <operation>equals</operation>
                <value>PEM</value>
            </criteriaItems>
            <criteriaItems>
                <field>NAM__c.SkipEM__c</field>
                <operation>equals</operation>
                <value>True</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>PEM - Closed Out</label>
        <name>PEMClosedOut</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>ClosedOut_SPCPartners</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>ClosedOutApprovalNotification</name>
                <type>Alert</type>
            </action>
            <action>
                <name>COTechApprovalDate</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>SPCTechTeam</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>NAM__c.ClosedOutStatus__c</field>
                <operation>equals</operation>
                <value>Tech Team</value>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>Teach Team - Closed Out</label>
        <name>TeachTeamClosedOut</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>ClosedOut_Approved</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>ClosedOutApprovalNotification</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>SPCPartners</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>NAM__c.ClosedOutStatus__c</field>
                <operation>equals</operation>
                <value>SPC Partners</value>
            </criteriaItems>
        </entryCriteria>
        <label>SPC Partners - Closed Out</label>
        <name>SPCPartnersClosedOut</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>NAM/ApprovalRequestNotificationClosedOut</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>NAM__c.Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>NAM__c.ClosedOutStatus__c</field>
            <operation>equals</operation>
            <value>Execution</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>ClosedOutApprovedbyAllNotification</name>
            <type>Alert</type>
        </action>
        <action>
            <name>COApprovedDate</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>ClosedOut_Execution</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>ClosedOutRejected</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>ClosesOut_EM</name>
